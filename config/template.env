# Supabase
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=
VITE_SUPABASE_SERVICE_ROLE=

# Ambiente
VITE_APP_ENV=development # development, homologacao, production
VITE_APP_URL=http://localhost:5173

# Configurações do App
VITE_APP_NAME=Gestão Profissional
VITE_APP_DESCRIPTION=Sistema de Gestão de Profissionais
VITE_APP_VERSION=1.0.0

# Configurações de API
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3

# Configurações de Cache
VITE_CACHE_TTL=3600
VITE_CACHE_PREFIX=gp_

# Configurações de Log
VITE_LOG_LEVEL=info
VITE_SENTRY_DSN=

# Configurações de Segurança
VITE_RATE_LIMIT_MAX_REQUESTS=100
VITE_RATE_LIMIT_WINDOW_MS=60000

# Configurações de Feature Flags
VITE_FEATURE_AI_CHAT=true
VITE_FEATURE_EXCEL_IMPORT=true
VITE_FEATURE_MANUAL_FORM=true 